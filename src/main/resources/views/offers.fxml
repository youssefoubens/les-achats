<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.youssef.gestion_achats.controller.OffreController">
    <children>
        <TableView fx:id="offreTable" layoutX="20.0" layoutY="20.0" prefHeight="400.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="idColumn" text="ID" />
                <TableColumn fx:id="descriptionColumn" text="Description" />
                <TableColumn fx:id="fournisseurColumn" text="Fournisseur" />
                <TableColumn fx:id="articleColumn" text="Article" />
                <TableColumn fx:id="sousArticleColumn" text="Sous-Article" />
                <TableColumn fx:id="sousSousArticleColumn" text="Sous-Sous-Article" />
            </columns>
        </TableView>
        <Button text="Go Back" onAction="#goback" layoutX="20.0" layoutY="430.0"/>
    </children>
</AnchorPane>
